{
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cmake.configureOnOpen": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "args": ["~/.vimrc"],
    "command": "source"
  },
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.largeFileOptimizations": false,
  "editor.lineNumbers": "relative",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "extensions.autoUpdate": false,
  "html.format.templating": true,
  "liveshare.anonymousGuestApproval": "reject",
  "liveshare.connectionMode": "relay",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "prettier.arrowParens": "always",
  "prettier.bracketSpacing": true,
  "prettier.enable": true,
  "rapid.cliRoot": "/usr/local/lib/node_modules/rde-cli",
  "security.workspace.trust.untrustedFiles": "open",
  "sonarlint.rules": {
    "python:S125": {
      "level": "off"
    },
    "python:S1186": {
      "level": "off"
    },
    "python:S117": {
      "level": "off"
    }
  },
  "telemetry.enableTelemetry": false,
  "todo-tree.general.tagGroups": {
    "FIXME": ["FIXME", "FIXIT", "FIX"]
  },
  "vim.easymotion": true,
  "vim.easymotionMarkerForegroundColorOneChar": "yellow",
  "vim.foldfix": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "after": ["<Esc>"],
      "before": ["j", "j"]
    }
  ],
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "after": ["d", "d"],
      "before": ["<leader>", "d"]
    },
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    }
  ],
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.easymotioninputmode": "#007ACC",
  "vim.statusBarColors.easymotionmode": "#007ACC",
  "vim.statusBarColors.insert": ["#FD0E35", "#FFFFFF"],
  "vim.statusBarColors.normal": ["#13AB45", "#FFFFFF"],
  "vim.statusBarColors.replace": "#D08770",
  "vim.statusBarColors.searchinprogressmode": ["#007ACC", "#FFFFFF"],
  "vim.statusBarColors.commandlineinprogress": ["#007ACC", "#FFFFFF"],
  "vim.statusBarColors.surroundinputmode": "#007ACC",
  "vim.statusBarColors.visual": ["#FF51D0", "#FFFFFF"],
  "vim.statusBarColors.visualblock": "#A3BE8C",
  "vim.statusBarColors.visualline": "#B48EAD",
  "vim.surround": true,
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.colorCustomizations": {
    "statusBar.background": "#8FBCBB",
    "statusBar.debuggingBackground": "#8FBCBB",
    "statusBar.debuggingForeground": "#434C5E",
    "statusBar.foreground": "#434C5E",
    "statusBar.noFolderBackground": "#8FBCBB"
  },
  "workbench.colorTheme": "Dracula Soft",
  "workbench.editor.enablePreview": false,
  "workbench.editor.untitled.hint": "hidden",
  "workbench.editorAssociations": {
    "*.exe": "default",
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.iconTheme": "material-icon-theme",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "editor.indentSize": "tabSize",
  "prettier.vueIndentScriptAndStyle": true,
  "editor.inlineSuggest.enabled": true,
  "git.openRepositoryInParentFolders": "always",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "window.zoomLevel": 1,
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "yaml.schemas": {
    "https://schemaservice-dev.dx.bloomberg.com/schemas/devise.yaml": "devise.yaml",
    "https://schemaservice-dev.dx.bloomberg.com/schemas/build.yaml": "build.yaml"
  },
  "extensions.ignoreRecommendations": true
}
